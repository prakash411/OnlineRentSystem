@model IEnumerable<renter.Models.Property>

<partial name="_toster" />

<div class="container">
    
</div>

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="custom-text">Your Selling Property List</h2>
        </div>
        <div class="col-6 text-right">
            <a asp-action="Upsert" class="btn custom-btn">
                <i class="fas fa-plus"></i> &nbsp; Sell New Property
            </a>
        </div>
    </div>

    <br /><br />

    @if (Model.Count() > 0)
    {
        <div class="row">
             @foreach (var prod in Model)
             {
                <partial name="_IndividualProductCard2" model="prod" />
             }
        </div>
    }
    else
    {
        <p> No product exists.</p>
    }

</div>

